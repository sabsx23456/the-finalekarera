{
    "functions": {
        "api/**/*.ts": {
            "runtime": "nodejs20.x"
        }
    },
    "rewrites": [
        {
            "source": "/wccstream/:path*",
            "destination": "https://stream.wccgames7.xyz/wccstream/:path*"
        },
        {
            "source": "/((?!assets/|api/|.*\\..*).*)",
            "destination": "/index.html"
        }
    ]
}
