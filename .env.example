VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here
VITE_OPENROUTER_MODEL=openai/gpt-5.2-chat
VITE_OPENROUTER_VISION_MODEL=google/gemini-3-flash-preview

# Optional (Karera AI Vision): full URL to your Supabase Edge Function / backend endpoint.
# If unset, the Karera live-board Vision polling is automatically disabled.
VITE_SUPABASE_FUNCTION_URL=https://YOUR_PROJECT_REF.functions.supabase.co/YOUR_FUNCTION

# Server-only (Vercel Serverless Functions under /api)
# Never expose these as VITE_*
OPENROUTER_API_KEY=your_openrouter_api_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Local scripts (example)
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=your_admin_password_here
